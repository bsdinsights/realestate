              name="Diện tích thực tế"
              sequence="12"
              action="bsd_dt_tt_action"
              parent="bsd_main_dich_vu_root"/>

    <record id="bsd_dich_vu.bsd_main_dich_vu_root" model="ir.ui.menu">
        <field name="action" eval="False"/>
        <field name="active" eval="True"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="name">Dịch vụ</field>
        <field name="parent_id" eval="False"/>
        <field name="sequence">10</field>
        <field name="web_icon">fa fa-th-list,#FFFFFF,#2F4F4F</field>
    </record>
</odoo>