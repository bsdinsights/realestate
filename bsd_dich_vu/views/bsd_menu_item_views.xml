                  name="Thay đổi đồng sở hữu"
                  sequence='10'
                  action='bsd_pl_dsh_action'
                  parent='bsd_phu_luc_menu' />
        <menuitem id="bsd_pl_tti_menu_action"
                  name="Thay đổi thông tin"
                  sequence='11'
                  action='bsd_pl_tti_action'
                  parent='bsd_phu_luc_menu' />
