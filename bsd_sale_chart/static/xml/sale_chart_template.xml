<?xml version="1.0" encoding="UTF-8" ?>

<templates xml:space="preserve">
    <div t-name="bsd_sale_chart.gio_hang">
        <div class="o_form_view">
            <div class="o_form_sheet_bg"/>
        </div>
    </div>
    <t t-name="bsd_sale_chart.data_filter">
        <div class="o_form_view">
            <div class="o_form_view">
                <div class="o_form_sheet_bg">
                    <div class="clearfix position_relative o_form_sheet">
                        <div class="row">
                            <table class="col-sm-3 o_group">
                                <tbody>
                                    <tr class="create_du_an_id">
                                        <td class="o_td_label" width="40%"><label class="o_form_label">Dự án</label></td>
                                        <td class="o_td_field" width="60%"></td>
                                    </tr>
                                </tbody>
                            </table>
                            <table class="col-sm-3 o_group">
                                <tbody>
                                    <tr class="create_dot_mb_id">
                                        <td class="o_td_label" width="40%"><label class="o_form_label">Đợt mở bán</label></td>
                                        <td class="o_td_field" width="60%"></td>
                                    </tr>
                                </tbody>
                            </table>
                            <table class="col-sm-3 o_group">
                                <tbody>
                                    <tr class="create_unit">
                                        <td class="o_td_label" width="40%"><label class="o_form_label">Mã căn hộ</label></td>
                                        <td class="o_td_field" width="60%"></td>
                                    </tr>
                                </tbody>
                            </table>
                            <table class="col-sm-3 o_group">
                                <tbody>
                                    <tr class="create_view">
                                        <td class="o_td_label" width="40%"><label class="o_form_label">View</label></td>
                                        <td class="o_td_field" width="60%"></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <div class="row">
                            <table class="col-sm-3 o_group">
                                <tbody>
                                    <tr class="create_tu_gia">
                                        <td class="o_td_label" width="40%"><label class="o_form_label">Giá từ</label></td>
                                        <td width="55%"><span class="o_td_field" ></span></td>
                                        <td width="5%"><span>đ</span></td>
                                    </tr>
                                </tbody>
                            </table>
                            <table class="col-sm-3 o_group">
                                <tbody>
                                    <tr class="create_den_gia">
                                        <td class="o_td_label" width="40%"><label class="o_form_label">Đến</label></td>
                                        <td width="55%"><span class="o_td_field" ></span></td>
                                        <td width="5%"><span>đ</span></td>
                                    </tr>
                                </tbody>
                            </table>
                            <table class="col-sm-3 o_group">
                                <tbody>
                                    <tr class="create_tu_dt">
                                        <td class="o_td_label" width="40%"><label class="o_form_label">Diện tích từ</label></td>
                                        <td width="55%"><span class="o_td_field" ></span></td>
                                        <td width="5%"><span>m2</span></td>
                                    </tr>
                                </tbody>
                            </table>
                            <table class="col-sm-3 o_group">
                                <tbody>
                                    <tr class="create_den_dt">
                                        <td class="o_td_label" width="40%"><label class="o_form_label">Đến</label></td>
                                        <td width="55%"><span class="o_td_field" ></span></td>
                                        <td width="5%"><span>m2</span></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <div class="row">
                            <table class="o_group col-6">
                                <tbody>
                                    <tr class="create_huong">
                                        <td class="o_td_label" width="40%"><label class="o_form_label">Hướng</label></td>
                                        <td width="60%"><span class="o_td_field" ></span></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <button class="btn btn-primary oe_right" id="search">Tìm</button>
                    </div>
                </div>
                <div id="chart">
                </div>
            </div>
        </div>
    </t>

    <t t-name="bsd_sale_chart.chart">
        <t t-foreach="data" t-as="toa">
            <div class="card">
                <t t-set="href" t-value="'#collapseToa'+ toa.headerToa[0]"/>
                <t t-set="hrefId" t-value="'collapseToa'+ toa.headerToa[0]"/>
                <div class="card-header bsd_header_toa" data-toggle="collapse" t-attf-href="{{href}}">
                    <i class="fa fa-search-plus"></i>
                    <a class="bsd_title" >
                        <span t-esc="toa.headerToa[1]"/>
                    </a>
                    <div class="d-inline-flex flex-row ml-4">
                        <div class="chuan_bi d-flex justify-content-center ml-2 bsd_state">
                            <div t-esc="toa.state.chuan_bi"></div>
                        </div>
                        <div class="san_sang d-flex justify-content-center ml-2 bsd_state">
                            <div t-esc="toa.state.san_sang"></div>
                        </div>
                        <div class="dat_cho d-flex justify-content-center ml-2 bsd_state">
                            <div t-esc="toa.state.dat_cho"></div>
                        </div>
                        <div class="giu_cho d-flex justify-content-center ml-2 bsd_state">
                            <div t-esc="toa.state.giu_cho"></div>
                        </div>
                        <div class="dat_coc d-flex justify-content-center ml-2 bsd_state">
                            <div t-esc="toa.state.dat_coc"></div>
                        </div>
                        <div class="chuyen_coc d-flex justify-content-center ml-2 bsd_state">
                            <div t-esc="toa.state.chuyen_coc"></div>
                        </div>
                        <div class="da_thu_coc d-flex justify-content-center ml-2 bsd_state">
                            <div t-esc="toa.state.da_thu_coc"></div>
                        </div>
                        <div class="hoan_tat_dat_coc d-flex justify-content-center ml-2 bsd_state">
                            <div t-esc="toa.state.hoan_tat_dat_coc"></div>
                        </div>
                        <div class="thanh_toan_dot_1 d-flex justify-content-center ml-2 bsd_state">
                            <div t-esc="toa.state.thanh_toan_dot_1"></div>
                        </div>
                        <div class="ky_thoa_thuan_coc d-flex justify-content-center ml-2 bsd_state">
                            <div t-esc="toa.state.ky_thoa_thuan_coc"></div>
                        </div>
                        <div class="du_dieu_kien d-flex justify-content-center ml-2 bsd_state">
                            <div t-esc="toa.state.du_dieu_kien"></div>
                        </div>
                        <div class="da_ban d-flex justify-content-center ml-2 bsd_state">
                            <div t-esc="toa.state.da_ban"></div>
                        </div>
                    </div>
                </div>
                <div t-attf-id="{{hrefId}}" class="collapse">
                    <t t-foreach="toa.detailToa" t-as="tang">
                        <div class="card">
                            <t t-set="hrefTang" t-value="'#collapseTang'+ tang.headerTang[0]"/>
                            <t t-set="hrefIdTang" t-value="'collapseTang'+ tang.headerTang[0]"/>
                            <div class="card-header bsd_header_tang" data-toggle="collapse" t-attf-data-target="{{hrefTang}}">
                                <i class="fa fa-plus"></i>
                                <a class="bsd_title ">
                                    <span t-esc="tang.headerTang[1]"/>
                                </a>
                                <div class="d-inline-flex flex-row ml-4">
                                    <div class="chuan_bi d-flex justify-content-center ml-2 bsd_state">
                                        <div t-esc="tang.state.chuan_bi"></div>
                                    </div>
                                    <div class="san_sang d-flex justify-content-center ml-2 bsd_state">
                                        <div t-esc="tang.state.san_sang"></div>
                                    </div>
                                    <div class="dat_cho d-flex justify-content-center ml-2 bsd_state">
                                        <div t-esc="tang.state.dat_cho"></div>
                                    </div>
                                    <div class="giu_cho d-flex justify-content-center ml-2 bsd_state">
                                        <div t-esc="tang.state.giu_cho"></div>
                                    </div>
                                    <div class="dat_coc d-flex justify-content-center ml-2 bsd_state">
                                        <div t-esc="tang.state.dat_coc"></div>
                                    </div>
                                    <div class="chuyen_coc d-flex justify-content-center ml-2 bsd_state">
                                        <div t-esc="tang.state.chuyen_coc"></div>
                                    </div>
                                    <div class="da_thu_coc d-flex justify-content-center ml-2 bsd_state">
                                        <div t-esc="tang.state.da_thu_coc"></div>
                                    </div>
                                    <div class="hoan_tat_dat_coc d-flex justify-content-center ml-2 bsd_state">
                                        <div t-esc="tang.state.hoan_tat_dat_coc"></div>
                                    </div>
                                    <div class="thanh_toan_dot_1 d-flex justify-content-center ml-2 bsd_state">
                                        <div t-esc="tang.state.thanh_toan_dot_1"></div>
                                    </div>
                                    <div class="ky_thoa_thuan_coc d-flex justify-content-center ml-2 bsd_state">
                                        <div t-esc="tang.state.ky_thoa_thuan_coc"></div>
                                    </div>
                                    <div class="du_dieu_kien d-flex justify-content-center ml-2 bsd_state">
                                        <div t-esc="tang.state.du_dieu_kien"></div>
                                    </div>
                                    <div class="da_ban d-flex justify-content-center ml-2 bsd_state">
                                        <div t-esc="tang.state.da_ban"></div>
                                    </div>
                                </div>
                            </div>
                            <div t-attf-id="{{hrefIdTang}}" class="collapse">
                                <div class="container-fluid mt-2">
                                        <t t-foreach="tang.detailTang" t-as="unit">
                                            <div class="d-inline-flex flex-row ml-2 mb-2">
                                                <div t-attf-class="{{unit[6]}} bsd_unit">
                                                    <div class="text-center justify-content-center">
                                                        <div class="bsd_title" t-esc="unit[5]"/>
                                                        <div>Giữ chỗ:<span t-esc="unit[7]"/></div>
                                                        <div class="bsd_gia"><span t-esc="unit[8]"/><span>đ</span></div>
                                                        <div>Quan tâm BĐS: 0</div>
                                                    </div>
                                                </div>
                                            </div>
                                        </t>
                                </div>
                            </div>
                        </div>
                    </t>
                </div>
            </div>
        </t>
    </t>
</templates>