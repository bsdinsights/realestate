<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-extend="ListView.buttons">
        <t t-jquery="button.o_list_button_add" t-operation="after">
            <button t-if="widget.modelName == 'bsd.tb_tt'"
                    class="btn btn-primary oe_tao_td_button"
                    type="button" accesskey="f">Tạo tự động</button>
        </t>
        <t t-jquery="button.o_list_button_add" t-operation="after">
            <button t-if="widget.modelName == 'bsd.tb_nn'"
                    class="btn btn-primary oe_tao_td_nn_button"
                    type="button" accesskey="f">Tạo tự động</button>
        </t>
    </t>
</templates>