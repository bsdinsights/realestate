<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!--Top menu-->
    <menuitem id="bsd_main_dich_vu_root" name="Dịch vụ"/>
    <menuitem id="bsd_hd_ban_menu_action"
              name="Hợp đồng mua bán"
              sequence='10'
              action='bsd_hd_ban_action'
              parent='bsd_main_dich_vu_root' />
    <menuitem id="bsd_phu_luc_menu"
              name="Phụ lục"
              sequence="11"
              parent="bsd_main_dich_vu_root"/>
